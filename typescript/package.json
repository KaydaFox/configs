{
  "name": "@kaydafox/typescript-config",
  "version": "1.0.0",
  "description": "A collection of typescript configs I have for different use cases",
  "main": "src/tsconfig.json",
  "author": "KaydaFox",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./src/tsconfig.json",
      "import": "./src/tsconfig.json",
      "require": "./src/tsconfig.json"
    },
    "./base": {
      "types": "./src/tsconfig.json",
      "import": "./src/tsconfig.json",
      "require": "./src/tsconfig.json"
    },
    "./no-decorators": {
      "types": "./src/no-decorators.json",
      "import": "./src/no-decorators.json",
      "require": "./src/no-decorators.json"
    },
    "./strict": {
      "types": "./src/strict.json",
      "import": "./src/strict.json",
      "require": "./src/strict.json"
    }
  },
  "sideEffects": false
}
